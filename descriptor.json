{
  "name": "NucleiSegmentation-MaskRCNN",
  "description": "Workflow for nuclei segmentation using Mask R-CNN",
  "container-image": {
    "image": "neubiaswg5/w_nucleisegmentation-maskrcnn",
    "type": "singularity"
  },
  "command-line": "python wrapper.py HOST PUBLIC_KEY PRIVATE_KEY PROJECT_ID SOFTWARE_ID NUCLEI_MAJOR_AXIS",
  "inputs": [
    {
      "id": "host",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "BIAFLOWS host",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "public_key",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "BIAFLOWS public key",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "private_key",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "BIAFLOWS private key",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "project_id",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "BIAFLOWS project ID",
      "set-by-server": true,
      "optional": false,
      "type": "Number"
    },
    {
      "id": "software_id",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "BIAFLOWS software ID",
      "set-by-server": true,
      "optional": false,
      "type": "Number"
    },
    {
      "id": "nuclei_major_axis",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Max estimated diameter of a nucleus",
      "description": "Max estimated diameter of a nucleus",
      "default-value": 64,
      "set-by-server": false,
      "optional": true,
      "type": "Number"
    }
  ],

  "schema-version": "cytomine-0.1"
}
